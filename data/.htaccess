# data 폴더 웹 접근 차단 (config 제외하고는 직접 접근 불가)
<FilesMatch "\.(php|sql|log|json)$">
    Order Allow,Deny
    Deny from all
</FilesMatch>

# config 폴더 내 PHP 파일은 include는 가능하지만 직접 실행 차단
<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteCond %{REQUEST_URI} !^/data/uploads/
    RewriteRule ^(.*)$ - [F,L]
</IfModule>
